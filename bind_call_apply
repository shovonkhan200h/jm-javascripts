const normalPerson = {
    name: 'John',
    lastName: 'alex',
    salary:2000
}


const babyPerson ={
        name: 'baby',
        lastName: 'alice',
        salary: 10000,
        getFullName:function(){
                console.log(this.name , this.lastName);
        },
        newSalary:function(tax, lawra, chanda){
              this.salary = this.salary - tax - lawra - chanda;
              return this.salary;
        }
}
